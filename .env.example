# Example environment configuration for Moonblokz Telemetry Hub
# Copy this file to .env and set your own values

# API key for probe authentication
SPIN_VARIABLE_PROBE_API_KEY=your-probe-api-key-here-32-bytes-minimum

# API key for log collector authentication
SPIN_VARIABLE_LOG_COLLECTOR_API_KEY=your-log-collector-api-key-here-32-bytes

# API key for CLI authentication
SPIN_VARIABLE_CLI_API_KEY=your-cli-api-key-here-32-bytes-minimum

# Minutes before old logs/commands are deleted (default: 30)
SPIN_VARIABLE_DELETE_TIMEOUT=30

# Default telemetry upload interval in seconds (default: 300)
SPIN_VARIABLE_DEFAULT_UPLOAD_INTERVAL=300
